<https://w3id.org/ontoim> rdf:type owl:Ontology ;
                           owl:imports <https://w3id.org/italia/onto/ACCO> ,
                                       <https://w3id.org/italia/onto/ADMS> ,
                                       <https://w3id.org/italia/onto/AccessCondition> ,
                                       <https://w3id.org/italia/onto/CLV> ,
                                       <https://w3id.org/italia/onto/COV> ,
                                       <https://w3id.org/italia/onto/CPEV> ,
                                       <https://w3id.org/italia/onto/CPSV> ,
                                       <https://w3id.org/italia/onto/CPV> ,
                                       <https://w3id.org/italia/onto/IoT> ,
                                       <https://w3id.org/italia/onto/MU> ,
                                       <https://w3id.org/italia/onto/POI> ,
                                       <https://w3id.org/italia/onto/POT> ,
                                       <https://w3id.org/italia/onto/PublicContract> ,
                                       <https://w3id.org/italia/onto/SM> ,
                                       <https://w3id.org/italia/onto/TI> ,
                                       <https://w3id.org/italia/onto/l0> ;
                           dct:accrualPeriodicity <http://publications.europa.eu/resource/authority/frequency/IRREG> ;
                           dct:created "2022-09-24"^^xsd:date ;
                           dct:creator <https://orcid.org/0000-0002-6249-0899> ;
                           dct:description "Questa ontologia, unita alle ontologie della rete italiana OntoPiA, permette di modellare la realtà di un Comune. Include eventi, associazioni, imprese, scuole, ospedali, aree verdi, piante, segnaletica stradale, osservazioni sul traffico, incidenti stradali e statistiche demografiche."@it ,
                                           "This ontology, combined with ontologies from the Italian OntoPiA network, makes it possible to model the reality of a municipality. It includes events, associations, businesses, schools, hospitals, green areas, plants, street signs, traffic observations, traffic accidents and demographic statistics."@en ;
                           dct:issued "2022-09-24"^^xsd:date ;
                           dct:language <http://publications.europa.eu/resource/authority/language/ENG> ,
                                        <http://publications.europa.eu/resource/authority/language/ITA> ;
                           dct:license <http://creativecommons.org/licenses/by/4.0> ;
                           dct:rightsHolder <https://orcid.org/0000-0002-6249-0899> ;
                           dct:title "Ontology for Italian Municipalities"@en ;
                           owl:versionInfo "0.1"^^rdfs:Literal ;
                           dcat:accessURL <https://github.com/luca-martinelli-09/ontoim/> ;
                           dcat:contactPoint <https://orcid.org/0000-0002-6249-0899> ;
                           dcat:downloadURL <https://raw.githubusercontent.com/luca-martinelli-09/ontoim/master/ontology/latest/OntoIM.rdf> ;
                           dcat:theme <http://publications.europa.eu/resource/authority/data-theme/TECH> ;
                           sdo:image <https://raw.githubusercontent.com/luca-martinelli-09/ontoim/master/ontology/latest/OntoMunicipality.svg> ;
