###  https://w3id.org/ontoim/hasCivilStatusCategory
:hasCivilStatusCategory rdf:type owl:ObjectProperty ;
                        rdfs:domain :CivilStatus ;
                        rdfs:range :CivilStatusCategory ;
                        rdfs:label "ha categoria di stato civile"@it ,
                                   "has civil status category"@en .


###  https://w3id.org/ontoim/hasDemographicObservation
:hasDemographicObservation rdf:type owl:ObjectProperty ;
                           owl:inverseOf :isDemographicObservationFor ;
                           rdfs:domain owl:Thing ;
                           rdfs:range owl:Thing ,
                                      :DemographicObservation ;
                           rdfs:label "ha osservazione demografica"@it ,
                                      "has demographic observation"@en .


###  https://w3id.org/ontoim/hasDemographicReference
:hasDemographicReference rdf:type owl:ObjectProperty ;
                         rdfs:domain :DemographicObservation ;
                         rdfs:range [ rdf:type owl:Class ;
                                      owl:unionOf ( cpvapit:Family
                                                    cpvapit:Person
                                                  )
                                    ] ;
                         rdfs:label "ha riferimento demografico"@it ,
                                    "has demographic reference"@en .


###  https://w3id.org/ontoim/hasSpatialDelimitation
:hasSpatialDelimitation rdf:type owl:ObjectProperty ;
                        rdfs:domain owl:Thing ;
                        rdfs:range clvapit:SpatialObject ;
                        rdfs:label "ha delimitazione spaziale"@it ,
                                   "has spatial delimitation"@en .


###  https://w3id.org/ontoim/hasTouristType
:hasTouristType rdf:type owl:ObjectProperty ;
                rdfs:domain :Tourists ;
                rdfs:range :TouristType ;
                rdfs:label "ha tipo di turista"@it ,
