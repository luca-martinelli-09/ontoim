###  https://w3id.org/ontoim/TouristType
:TouristType rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Class ;
                                   owl:oneOf ( :Arrival
                                               :Presence
                                             )
                                 ] ;
             rdfs:comment "Questa classe è una classe numerata che rappresenta la tipologia di turista: arrivo o presenza."@it ,
                          "This class is an enumerated class representing the typology of tourist: arrival or presence."@en ;
             rdfs:label "Tipo di Turista"@it ,
                        "Tourist Type"@en .


###  https://w3id.org/ontoim/Tourists
:Tourists rdf:type owl:Class ;
          rdfs:subClassOf :DemographicObservation ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasTouristType ;
                            owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :TouristType
                          ] ;
          rdfs:comment "Questa classe rappresenta un'osservazione demografica sui turisti. Le osservazioni sui turisti è divisa in arrivi e presenze."@it ,
                       "This class represents the demographic observation for tourism. Tourism observation is divided in arrivals and presences."@en ;
          rdfs:label "Tourists"@en ,
                     "Turisti"@it .


