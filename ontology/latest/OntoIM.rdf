        <dcat:downloadURL rdf:resource="https://raw.githubusercontent.com/luca-martinelli-09/ontoim/master/ontology/latest/OntoIM.rdf"/>
        <dcat:theme rdf:resource="http://publications.europa.eu/resource/authority/data-theme/TECH"/>
