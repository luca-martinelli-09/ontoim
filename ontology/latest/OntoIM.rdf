        <dct:accrualPeriodicity rdf:resource="http://publications.europa.eu/resource/authority/frequency/IRREG"/>
        <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-09-24</dct:created>
        <dct:creator rdf:resource="https://orcid.org/0000-0002-6249-0899"/>
        <dct:description xml:lang="it">Questa ontologia, unita alle ontologie della rete italiana OntoPiA, permette di modellare la realtà di un Comune. Include eventi, associazioni, imprese, scuole, ospedali, aree verdi, piante, segnaletica stradale, osservazioni sul traffico, incidenti stradali e statistiche demografiche.</dct:description>
        <dct:description xml:lang="en">This ontology, combined with ontologies from the Italian OntoPiA network, makes it possible to model the reality of a municipality. It includes events, associations, businesses, schools, hospitals, green areas, plants, street signs, traffic observations, traffic accidents and demographic statistics.</dct:description>
        <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-09-24</dct:issued>
        <dct:language rdf:resource="http://publications.europa.eu/resource/authority/language/ENG"/>
        <dct:language rdf:resource="http://publications.europa.eu/resource/authority/language/ITA"/>
        <dct:license rdf:resource="http://creativecommons.org/licenses/by/4.0"/>
        <dct:rightsHolder rdf:resource="https://orcid.org/0000-0002-6249-0899"/>
        <dct:title xml:lang="en">Ontology for Italian Municipalities</dct:title>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">0.1</owl:versionInfo>
        <dcat:accessURL rdf:resource="https://github.com/luca-martinelli-09/ontoim/"/>
