        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CLV/Address"/>
        <rdfs:comment xml:lang="it">Questa proprietà rappresenta l&apos;indirizzo di un&apos;unità locale o sede staccata di un&apos;organizzazione.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property represents the address of a local unit or branch office of an organization.</rdfs:comment>
        <rdfs:label xml:lang="it">ha indirizzo dell&apos;unità locale</rdfs:label>
        <rdfs:label xml:lang="en">has local unit address</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontoim/hasSpatialDelimitation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ontoim/hasSpatialDelimitation">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CLV/SpatialObject"/>
        <rdfs:label xml:lang="it">ha delimitazione spaziale</rdfs:label>
        <rdfs:label xml:lang="en">has spatial delimitation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontoim/hasTouristType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ontoim/hasTouristType">
