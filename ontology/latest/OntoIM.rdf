    <!-- https://w3id.org/ontoim/hasCivilStatusCategory -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ontoim/hasCivilStatusCategory">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/ontoim/CivilStatus"/>
                    <rdf:Description rdf:about="https://w3id.org/ontoim/CivilStatusEvent"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/ontoim/CivilStatusCategory"/>
        <rdfs:label xml:lang="it">ha categoria di stato civile</rdfs:label>
        <rdfs:label xml:lang="en">has civil status category</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontoim/hasDemographicEvent -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ontoim/hasDemographicEvent">
        <owl:inverseOf rdf:resource="https://w3id.org/ontoim/isDemographicEventFor"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/ontoim/DemographicEvent"/>
        <rdfs:label xml:lang="it">ha evento demografico</rdfs:label>
        <rdfs:label xml:lang="en">has demographic event</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontoim/hasDemographicObservation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ontoim/hasDemographicObservation">
        <owl:inverseOf rdf:resource="https://w3id.org/ontoim/isDemographicObservationFor"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/ontoim/DemographicObservation"/>
        <rdfs:label xml:lang="it">ha osservazione demografica</rdfs:label>
        <rdfs:label xml:lang="en">has demographic observation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontoim/hasDemographicReference -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ontoim/hasDemographicReference">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/ontoim/DemographicEvent"/>
                    <rdf:Description rdf:about="https://w3id.org/ontoim/DemographicObservation"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/italia/onto/CPV/Family"/>
                    <rdf:Description rdf:about="https://w3id.org/italia/onto/CPV/Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label xml:lang="it">ha riferimento demografico</rdfs:label>
        <rdfs:label xml:lang="en">has demographic reference</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontoim/hasEmployees -->

    <owl:ObjectProperty rdf:about="https://w3id.org/ontoim/hasEmployees">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/ontoim/hasDemographicObservation"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/COV/Organization"/>
        <rdfs:range rdf:resource="https://w3id.org/ontoim/Employees"/>
        <rdfs:label xml:lang="it">ha dipendenti</rdfs:label>
        <rdfs:label xml:lang="en">has employees</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontoim/hasLocalUnitAddress -->

