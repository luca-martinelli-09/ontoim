    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->


    


    <!-- https://w3id.org/italia/onto/ACCO/hasOfferedServiceDescription -->


    <owl:ObjectProperty rdf:about="https://w3id.org/italia/onto/ACCO/hasOfferedServiceDescription"/>
    


    <!-- https://w3id.org/ontoim/hasCivilStatusCategory -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontoim/hasCivilStatusCategory">
        <rdfs:domain rdf:resource="https://w3id.org/ontoim/CivilStatus"/>
        <rdfs:range rdf:resource="https://w3id.org/ontoim/CivilStatusCategory"/>
        <rdfs:label xml:lang="it">ha categoria di stato civile</rdfs:label>
        <rdfs:label xml:lang="en">has civil status category</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontoim/hasDemographicObservation -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontoim/hasDemographicObservation">
        <owl:inverseOf rdf:resource="https://w3id.org/ontoim/isDemographicObservationFor"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/ontoim/DemographicObservation"/>
        <rdfs:label xml:lang="it">ha osservazione demografica</rdfs:label>
        <rdfs:label xml:lang="en">has demographic observation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontoim/hasDemographicReference -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontoim/hasDemographicReference">
        <rdfs:domain rdf:resource="https://w3id.org/ontoim/DemographicObservation"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/italia/onto/CPV/Family"/>
                    <rdf:Description rdf:about="https://w3id.org/italia/onto/CPV/Person"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:label xml:lang="it">ha riferimento demografico</rdfs:label>
        <rdfs:label xml:lang="en">has demographic reference</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontoim/hasSpatialDelimitation -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontoim/hasSpatialDelimitation">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CLV/SpatialObject"/>
        <rdfs:label xml:lang="it">ha delimitazione spaziale</rdfs:label>
        <rdfs:label xml:lang="en">has spatial delimitation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontoim/hasTouristType -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontoim/hasTouristType">
        <rdfs:domain rdf:resource="https://w3id.org/ontoim/Tourists"/>
        <rdfs:range rdf:resource="https://w3id.org/ontoim/TouristType"/>
        <rdfs:label xml:lang="it">ha tipo di turista</rdfs:label>
        <rdfs:label xml:lang="en">has tourist type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontoim/isDemographicObservationFor -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontoim/isDemographicObservationFor">
        <rdfs:domain rdf:resource="https://w3id.org/ontoim/DemographicObservation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:label xml:lang="en">is demographic observation for</rdfs:label>
        <rdfs:label xml:lang="it">è osservazione demografica per</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontoim/ownedBy -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontoim/ownedBy">
        <owl:inverseOf rdf:resource="https://w3id.org/ontoim/ownsAccessiblePoI"/>
        <rdfs:domain rdf:resource="https://w3id.org/ontoim/AccessiblePointOfInterest"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/COV/Organization"/>
        <rdfs:label xml:lang="en">owned by</rdfs:label>
        <rdfs:label xml:lang="it">posseduto da</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/ontoim/ownsAccessiblePoI -->


    <owl:ObjectProperty rdf:about="https://w3id.org/ontoim/ownsAccessiblePoI">
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/COV/Organization"/>
        <rdfs:range rdf:resource="https://w3id.org/ontoim/AccessiblePointOfInterest"/>
        <rdfs:label xml:lang="en">owns accessible point of interest</rdfs:label>
        <rdfs:label xml:lang="it">possiede un punto di interesse accessibile</rdfs:label>
    </owl:ObjectProperty>
    


