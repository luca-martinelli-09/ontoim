    <!-- http://org.semanticweb.owlapi/error#Error2 -->


    <owl:Class rdf:about="http://org.semanticweb.owlapi/error#Error2"/>
    


    <!-- https://w3id.org/italia/onto/ACCO/OfferedServiceDescription -->


    <owl:Class rdf:about="https://w3id.org/italia/onto/ACCO/OfferedServiceDescription"/>
    


    <!-- https://w3id.org/italia/onto/CPV/Family -->


    <owl:Class rdf:about="https://w3id.org/italia/onto/CPV/Family"/>
    


    <!-- https://w3id.org/italia/onto/CPV/Person -->


    <owl:Class rdf:about="https://w3id.org/italia/onto/CPV/Person"/>
    


    <!-- https://w3id.org/ontoim/AccessiblePointOfInterest -->


    <owl:Class rdf:about="https://w3id.org/ontoim/AccessiblePointOfInterest">
        <rdfs:subClassOf rdf:resource="https://w3id.org/italia/onto/POI/PointOfInterest"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/italia/onto/ACCO/hasOfferedServiceDescription"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/italia/onto/ACCO/OfferedServiceDescription"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/italia/onto/AccessCondition/hasAccessCondition"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/italia/onto/AccessCondition/AccessCondition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ontoim/ownedBy"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/italia/onto/COV/Organization"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/italia/onto/SM/hasOnlineContactPoint"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/italia/onto/SM/OnlineContactPoint"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/italia/onto/SM/hasPhysicalContactPoint"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/italia/onto/SM/PhysicalContactPoint"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">An Accessible Point of Interest is a Point of Interest, usually a building, a shopping center, a pharmacy, etc.... It inherits the properties of OntoPiA&apos;s Point of Interest, but it also has an online or physical contact point, access conditions, offered services descriptions and an owner (Organization).</rdfs:comment>
        <rdfs:comment xml:lang="it">Un Punto di Interesse Accessibile è un Punto di Interesse, solitamente un edificio, un centro commerciale, una farmacia ecc.... Eredita le proprietà del Punto di Interesse di OntoPiA, ma possiede anche un punto di contatto online o fisico, una o più condizioni di acceso, può avere dei servizi generici e ha un possessore (Organizzazione).</rdfs:comment>
        <rdfs:label xml:lang="en">Accessible Point of Interest</rdfs:label>
        <rdfs:label xml:lang="it">Punto di Interesse Accessibile</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ontoim/Bookings -->


    <owl:Class rdf:about="https://w3id.org/ontoim/Bookings">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ontoim/DemographicObservation"/>
        <rdfs:comment xml:lang="it">Questa classe rappresenta un&apos;osservazione demografica sui turisti prenotati in una struttura ricettiva.</rdfs:comment>
        <rdfs:comment xml:lang="en">This class represents a demographic observation on the tourists booked in an accommodation facility.</rdfs:comment>
        <rdfs:label xml:lang="en">Bookings</rdfs:label>
        <rdfs:label xml:lang="it">Prenotazioni</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ontoim/Citizens -->


    <owl:Class rdf:about="https://w3id.org/ontoim/Citizens">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ontoim/DemographicObservation"/>
        <rdfs:comment xml:lang="it">Questa classe rappresenta un&apos;osservazione demografica fatta sui cittadini. Ad esempio, il numero di cittadini presenti nel comune.</rdfs:comment>
        <rdfs:comment xml:lang="en">This class represents a demographic observation made on the citizens. E.g. the number of citizens in the municipality.</rdfs:comment>
        <rdfs:label xml:lang="en">Citizens</rdfs:label>
        <rdfs:label xml:lang="it">Cittadini</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ontoim/CivilStatus -->


    <owl:Class rdf:about="https://w3id.org/ontoim/CivilStatus">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ontoim/DemographicObservation"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ontoim/hasCivilStatusCategory"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/ontoim/CivilStatusCategory"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="it">Questa classe rappresenta il numero di eventi di stato civile (nascite, matrimoni, morti, ecc...)</rdfs:comment>
        <rdfs:comment xml:lang="en">This class represents the number of civil status events and their types (marriages, births, deaths, etc...)</rdfs:comment>
        <rdfs:label xml:lang="en">Civil Status</rdfs:label>
        <rdfs:label xml:lang="it">Stato Civile</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ontoim/CivilStatusCategory -->


    <owl:Class rdf:about="https://w3id.org/ontoim/CivilStatusCategory">
        <rdfs:subClassOf rdf:resource="https://w3id.org/italia/onto/l0/Topic"/>
        <rdfs:comment xml:lang="it">Questa classe rappresenta la categoria dell&apos;evento di stato civile. Gli elementi di questa classe sono di tipo skos:Concept e devono essere definiti secondo il vocabolario controllato disponibile a https://w3id.org/ontoim/controlled-vocabulary/civil-status-categories</rdfs:comment>
        <rdfs:comment xml:lang="en">This class represents the category of the civil status event. The elements of this class are skos:Concept and must be defined according to the controlled vocabulary available at https://w3id.org/ontoim/controlled-vocabulary/civil-status-categories</rdfs:comment>
        <rdfs:label xml:lang="it">Categoria di Stato Civile</rdfs:label>
        <rdfs:label xml:lang="en">Civil Status Category</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ontoim/DemographicObservation -->


    <owl:Class rdf:about="https://w3id.org/ontoim/DemographicObservation">
        <rdfs:subClassOf rdf:resource="http://org.semanticweb.owlapi/error#Error2"/>
        <rdfs:subClassOf rdf:resource="https://w3id.org/italia/onto/l0/Activity"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ontoim/hasDemographicReference"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="https://w3id.org/italia/onto/CPV/Family"/>
                            <rdf:Description rdf:about="https://w3id.org/italia/onto/CPV/Person"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:onClass>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ontoim/hasSpatialDelimitation"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="https://w3id.org/italia/onto/CLV/SpatialObject"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/ontoim/value"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">A demographic observation is an observation made on the population of an entity, which can be the municipality, an organization, an event... The value defines the quantity of people or families observed, while the demographic reference represents the characteristics of the people observed. The demographic observation has also time and space delimitation. An example can be the number of female population of the municipality in 2021, or the number of italian citizens for a specific geographical area in 2020.</rdfs:comment>
        <rdfs:comment xml:lang="it">Un&apos;osservazione demografica è un&apos;osservazione fatta sulla popolazione di un&apos;entità, che può essere il comune, un&apos;organizzazione o un evento. Il valore definisce la quantità di persone o famiglie osservate, mentre il riferimento demografico rappresenta le caratteristiche osservate. L&apos;osservazione demografica ha inoltre una un tempo e uno spazio limitato. Ad esempio, può essere il numero di donne nel comune nel 2021, o il numero di cittadini italiani in una specifica area geografica nel 2020.</rdfs:comment>
        <rdfs:label xml:lang="en">Demographic Observation</rdfs:label>
        <rdfs:label xml:lang="it">Osservazione Demografica</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ontoim/Employees -->


    <owl:Class rdf:about="https://w3id.org/ontoim/Employees">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ontoim/DemographicObservation"/>
        <rdfs:comment xml:lang="it">Questa classe rappresenta un&apos;osservazione demografica sui dipendenti di un&apos;impresa.</rdfs:comment>
        <rdfs:comment xml:lang="en">This class represents a demographic observation on the employees of an organization.</rdfs:comment>
        <rdfs:label xml:lang="it">Dipendenti</rdfs:label>
        <rdfs:label xml:lang="en">Employees</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ontoim/Members -->


    <owl:Class rdf:about="https://w3id.org/ontoim/Members">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ontoim/DemographicObservation"/>
        <rdfs:comment xml:lang="it">Questa classe rappresenta un&apos;osservazione demografica sui membri di un&apos;associazione.</rdfs:comment>
        <rdfs:comment xml:lang="en">This class represents a demographic observation on the members of an association.</rdfs:comment>
        <rdfs:label xml:lang="en">Members</rdfs:label>
        <rdfs:label xml:lang="it">Membri</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ontoim/Subscribers -->


    <owl:Class rdf:about="https://w3id.org/ontoim/Subscribers">
        <rdfs:subClassOf rdf:resource="https://w3id.org/ontoim/DemographicObservation"/>
        <rdfs:comment xml:lang="it">Questa classe rappresenta un&apos;osservazione demografica sugli iscritti ad un evento, una scuola o un corso.</rdfs:comment>
        <rdfs:comment xml:lang="en">This class represents a demographic observation on the subscrivers to an event, a school or a course.</rdfs:comment>
        <rdfs:label xml:lang="it">Iscritti</rdfs:label>
        <rdfs:label xml:lang="en">Subscribers</rdfs:label>
    </owl:Class>
    


    <!-- https://w3id.org/ontoim/TouristType -->


    <owl:Class rdf:about="https://w3id.org/ontoim/TouristType">
        <owl:equivalentClass>
            <owl:Class>
                <owl:oneOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/ontoim/Arrival"/>
