    <!-- http://www.w3.org/2002/07/owl#maxQualifiedCardinality -->


    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#maxQualifiedCardinality"/>
    


    <!-- https://schema.org/affiliation -->


    <owl:AnnotationProperty rdf:about="https://schema.org/affiliation"/>
    


    <!-- https://schema.org/email -->


    <owl:AnnotationProperty rdf:about="https://schema.org/email"/>
    


