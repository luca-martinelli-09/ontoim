<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/ontoim#"
     xml:base="https://w3id.org/ontoim"
     xmlns:l0="https://w3id.org/italia/onto/l0/"
     xmlns:ns="http://www.w3.org/2006/vcard/ns#"
     xmlns:CLV="https://w3id.org/italia/onto/CLV/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:ADMS="https://w3id.org/italia/onto/ADMS/"
     xmlns:dcat="http://www.w3.org/ns/dcat#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:schema="https://schema.org/">
    <owl:Ontology rdf:about="https://w3id.org/ontoim">
        <owl:imports rdf:resource="https://w3id.org/italia/onto/ACCO"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/ADMS"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/AccessCondition"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/CLV"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/COV"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/CPEV"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/CPSV"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/CPV"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/IoT"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/MU"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/POI"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/POT"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/PublicContract"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/SM"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/TI"/>
        <owl:imports rdf:resource="https://w3id.org/italia/onto/l0"/>
        <terms:accrualPeriodicity rdf:resource="http://publications.europa.eu/resource/authority/frequency/IRREG"/>
        <terms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-09-24</terms:created>
        <terms:creator rdf:resource="https://orcid.org/0000-0002-6249-0899"/>
        <terms:description xml:lang="it">Questa ontologia, unita alle ontologie della rete italiana OntoPiA, permette di modellare la realtà di un Comune. Include eventi, associazioni, imprese, scuole, ospedali, aree verdi, piante, segnaletica stradale, osservazioni sul traffico, incidenti stradali e statistiche demografiche.</terms:description>
        <terms:description xml:lang="en">This ontology, combined with ontologies from the Italian OntoPiA network, makes it possible to model the reality of a municipality. It includes events, associations, businesses, schools, hospitals, green areas, plants, street signs, traffic observations, traffic accidents and demographic statistics.</terms:description>
        <terms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-09-24</terms:issued>
        <terms:language rdf:resource="http://publications.europa.eu/resource/authority/language/ENG"/>
        <terms:language rdf:resource="http://publications.europa.eu/resource/authority/language/ITA"/>
        <terms:license rdf:resource="http://creativecommons.org/licenses/by/4.0"/>
        <terms:rightsHolder rdf:resource="https://orcid.org/0000-0002-6249-0899"/>
        <terms:title xml:lang="en">Ontology for Italian Municipalities</terms:title>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">0.1</owl:versionInfo>
        <dcat:accessURL rdf:resource="https://github.com/luca-martinelli-09/ontoim/"/>
        <dcat:contactPoint rdf:resource="https://orcid.org/0000-0002-6249-0899"/>
        <dcat:downloadURL rdf:resource="https://raw.githubusercontent.com/luca-martinelli-09/ontoim/master/ontology/latest/OntoIM.rdf"/>
        <dcat:theme rdf:resource="http://publications.europa.eu/resource/authority/data-theme/TECH"/>
        <schema:image rdf:resource="https://raw.githubusercontent.com/luca-martinelli-09/ontoim/master/ontology/latest/OntoMunicipality.svg"/>
        <ADMS:acronym>OntoIM</ADMS:acronym>
        <ADMS:prefix>ontoim</ADMS:prefix>
        <ADMS:target xml:lang="en">Citizens</ADMS:target>
        <ADMS:target xml:lang="it">Cittadini</ADMS:target>
        <ADMS:target xml:lang="en">Companies</ADMS:target>
        <ADMS:target xml:lang="it">Imprese</ADMS:target>
        <ADMS:target xml:lang="it">Pubbliche Amministrazioni</ADMS:target>
        <ADMS:target xml:lang="en">Public Organizations</ADMS:target>
    </owl:Ontology>
    


