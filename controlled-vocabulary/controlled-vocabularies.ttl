<https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.4.8> a skos:Concept,
        ontoim:RoadSignalType ;
    rdfs:label "Inizio e fine regione" ;
    dct:identifier "V.3.4.8" ;
    skos:broader <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.4> ;
    skos:definition "Indica l'inizio di una regione e la fine di un'altra regione in viabilità autostradale." ;
    skos:example "https://it.wikipedia.org/wiki/File:Italian_traffic_signs_-_inizio_regione_autostrade.svg" ;
    skos:inScheme <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types> ;
    skos:prefLabel "Inizio e fine regione" .

<https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.5.1> a skos:Concept,
        ontoim:RoadSignalType ;
    rdfs:label "Direzione per autostrada" ;
    dct:identifier "V.3.5.1" ;
    skos:broader <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.5> ;
    skos:definition "Indica la direzione da seguire per imboccare l'autostrada indicata." ;
    skos:example "https://it.wikipedia.org/wiki/File:Italian_traffic_signs_-_direzione_autostrada.svg" ;
    skos:inScheme <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types> ;
    skos:prefLabel "Direzione per autostrada" .

<https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.5.2> a skos:Concept,
        ontoim:RoadSignalType ;
    rdfs:label "Segnale di conferma" ;
    dct:identifier "V.3.5.2" ;
    skos:broader <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.5> ;
    skos:definition "Indica le prossime località raggiungibili e la distanza da esse." ;
    skos:example "https://it.wikipedia.org/wiki/File:Italian_traffic_signs_-_conferma_autostradale.svg" ;
    skos:inScheme <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types> ;
