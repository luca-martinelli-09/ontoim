    skos:example "https://it.wikipedia.org/wiki/File:Italian_traffic_signs_-_intersezione_con_diritto_di_precedenza.svg" ;
    skos:inScheme <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types> ;
    skos:prefLabel "Intersezione con diritto di precedenza" .

