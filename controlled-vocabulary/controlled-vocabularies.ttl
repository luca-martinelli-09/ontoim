    skos:broader <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.7> ;
    skos:definition "Indica l'inizio del territorio comunale." ;
    skos:example "https://it.wikipedia.org/wiki/File:Italian_traffic_signs_-_inizio_territorio_comunale.svg" ;
    skos:inScheme <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types> ;
    skos:prefLabel "Inizio territorio comunale" .

<https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.7.10> a skos:Concept,
        ontoim:RoadSignalType ;
    rdfs:label "Direzione alberghiera" ;
    dct:identifier "V.3.7.10" ;
    skos:broader <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.7> ;
    skos:definition "Informa sulla direzione da intraprendere e sulla denominazione degli alberghi." ;
    skos:example "https://it.wikipedia.org/wiki/File:Italian_traffic_signs_-_direzione_alberghiera.svg" ;
    skos:inScheme <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types> ;
    skos:prefLabel "Direzione alberghiera" .

<https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.7.2> a skos:Concept,
        ontoim:RoadSignalType ;
    rdfs:label "Fiume" ;
    dct:identifier "V.3.7.2" ;
    skos:broader <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.7> ;
    skos:definition "Indica il fiume, con relativo nome, che si incontra." ;
    skos:example "https://it.wikipedia.org/wiki/File:Italian_traffic_signs_-_fiume.svg" ;
    skos:inScheme <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types> ;
    skos:prefLabel "Fiume" .

<https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.7.3> a skos:Concept,
        ontoim:RoadSignalType ;
    rdfs:label "Segnali di territorio" ;
    dct:identifier "V.3.7.3" ;
    skos:broader <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.7> ;
    skos:definition "Sono posti in posizione autonoma e singola, come segnali di direzione isolati, o come segnali di localizzazione, ma in tal caso non devono interferire con l'avvistamento e la visibilità dei segnali di pericolo, di prescrizione e di indicazione. Se impiegati, devono essere installati unicamente sulle strade che conducono direttamente al luogo segnalato e, salvo casi di impossibilità, a non oltre 10 km di distanza dal luogo." ;
    skos:example "https://it.wikipedia.org/wiki/File:Italian_traffic_signs_-_segnali_di_territorio.svg" ;
    skos:inScheme <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types> ;
    skos:prefLabel "Segnali di territorio" .

<https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.7.4> a skos:Concept,
        ontoim:RoadSignalType ;
    rdfs:label "Segnali turistici" ;
    dct:identifier "V.3.7.4" ;
    skos:broader <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.7> ;
    skos:definition "Sono posti in posizione autonoma e singola, come segnali di direzione isolati, o come segnali di localizzazione, ma in tal caso non devono interferire con l'avvistamento e la visibilità dei segnali di pericolo, di prescrizione e di indicazione. Se impiegati, devono essere installati unicamente sulle strade che conducono direttamente al luogo segnalato e, salvo casi di impossibilità, a non oltre 10 km di distanza dal luogo." ;
    skos:example "https://it.wikipedia.org/wiki/File:Italian_traffic_signs_-_segnali_turistici.svg" ;
    skos:inScheme <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types> ;
    skos:prefLabel "Segnali turistici" .

<https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.7.5> a skos:Concept,
        ontoim:RoadSignalType ;
    rdfs:label "Avvio alla zona industriale" ;
    dct:identifier "V.3.7.5" ;
    skos:broader <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.7> ;
    skos:definition "Nessuna indicazione di attività singola può essere inserita sui preavvisi di intersezione, sui segnali di preselezione, sui segnali di direzione, su quelli di conferma. Può essere invece installato nelle intersezioni e combinato, ove necessario col \"gruppo segnaletico unitario\" ivi esistente, il segnale di direzione con l'indicazione di \"zona industriale, zona artigianale, zona commerciale\"." ;
    skos:example "https://it.wikipedia.org/wiki/File:Italian_traffic_signs_-_avvio_zona_industriale.svg" ;
    skos:inScheme <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types> ;
    skos:prefLabel "Avvio alla zona industriale" .

<https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.7.6> a skos:Concept,
        ontoim:RoadSignalType ;
    rdfs:label "Direzione per le industrie" ;
    dct:identifier "V.3.7.6" ;
    skos:broader <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.7> ;
    skos:definition "Nei centri abitati, ove la zona o le zone industriali, artigianali o commerciali sono ben localizzate, si deve fare uso di segnali indicanti collettivamente la zona; tutte le attività e gli insediamenti particolari saranno indicati successivamente sulle intersezioni locali a valle degli itinerari principali di avvio alla \"zona industriale\" o \"zona artigianale\" o \"zona commerciale\" in genere." ;
    skos:example "https://it.wikipedia.org/wiki/File:Italian_traffic_signs_-_direzione_per_le_industrie.svg" ;
    skos:inScheme <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types> ;
    skos:prefLabel "Direzione per le industrie" .

