    skos:inScheme <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types> ;
    skos:prefLabel "Fine del diritto di precedenza" .

<https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.2.1.9> a skos:Concept,
        ontoim:RoadSignalType ;
