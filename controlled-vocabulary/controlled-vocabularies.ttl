    skos:inScheme <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types> ;
    skos:prefLabel "Dare precedenza nei sensi unici alternati" .

<https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.2.1.7> a skos:Concept,
        ontoim:RoadSignalType ;
    rdfs:label "Diritto di precedenza" ;
