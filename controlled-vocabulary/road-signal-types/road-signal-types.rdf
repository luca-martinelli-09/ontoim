    <dct:identifier>V.3.8.1.23</dct:identifier>
    <rdfs:label>Fine zona a velocità limitata</rdfs:label>
    <skos:prefLabel>Fine zona a velocità limitata</skos:prefLabel>
    <skos:definition>Indica la fine di una zona a velocità limitata.</skos:definition>
    <skos:example>https://it.wikipedia.org/wiki/File:Italian_traffic_signs_-_fine_zona_30.svg</skos:example>
    <skos:inScheme rdf:resource="https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/"/>
