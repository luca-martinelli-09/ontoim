    <dct:identifier>V.2.2.2.15</dct:identifier>
    <rdfs:label>Divieto di transito ai veicoli che trasportano esplosivo o prodotti facilmente infiammabili</rdfs:label>
    <skos:prefLabel>Divieto di transito ai veicoli che trasportano esplosivo o prodotti facilmente infiammabili</skos:prefLabel>
    <skos:definition>Vieta il transito ai veicoli che trasportano esplosivo o prodotti facilmente infiammabili.</skos:definition>
    <skos:example>https://it.wikipedia.org/wiki/File:Italian_traffic_signs_-_d…eto_di_transito_ai_veicoli_con_esplosivi_od_infiammabili.svg</skos:example>
    <skos:inScheme rdf:resource="https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/"/>
    <skos:broader rdf:resource="https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.2.2.2"/>
