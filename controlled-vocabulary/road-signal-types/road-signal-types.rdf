    <rdf:type rdf:resource="https://w3id.org/ontoim/RoadSignalType"/>
    <dct:identifier>V.3.8.2.4</dct:identifier>
    <rdfs:label>Strada chiusa al traffico</rdfs:label>
    <skos:prefLabel>Strada chiusa al traffico</skos:prefLabel>
    <skos:definition>Indica che il tratto di strada in questione è chiuso al transito veicolare.</skos:definition>
    <skos:example>https://it.wikipedia.org/wiki/File:Italian_traffic_signs_-_transitabilit%C3%A0_4.svg</skos:example>
    <skos:inScheme rdf:resource="https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/"/>
    <skos:broader rdf:resource="https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.8.2"/>
