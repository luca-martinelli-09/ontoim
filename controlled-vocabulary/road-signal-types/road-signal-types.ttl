    skos:example "https://it.wikipedia.org/wiki/File:Italian_traffic_signs_-_segnale_sperimentale_micromobilita_elettrica.svg" ;
    skos:inScheme <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types> ;
    skos:prefLabel "Inizio centro abitato con sperimentazione della micromobilità elettrica" .

