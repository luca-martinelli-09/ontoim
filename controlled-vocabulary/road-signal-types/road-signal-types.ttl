    skos:broader <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.8> ;
    skos:definition "Indica la fine di un'autostrada." ;
    skos:example "https://it.wikipedia.org/wiki/File:Italian_traffic_signs_-_segnale_fine_autostrada.svg" ;
    skos:inScheme <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types> ;
