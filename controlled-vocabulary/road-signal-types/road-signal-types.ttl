    skos:inScheme <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types> ;
    skos:prefLabel "SOS" .

<https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.8.1.40> a skos:Concept,
        ontoim:RoadSignalType ;
