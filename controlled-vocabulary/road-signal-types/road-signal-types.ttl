<https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.1.1.II.2.85> a skos:Concept,
        ontoim:RoadSignalType ;
    rdfs:label "Dosso " ;
    dct:identifier "V.1.1.II.2.85" ;
