    dct:identifier "V.3.8.1.40" ;
    skos:broader <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.8> ;
    skos:definition "Segnala il termine del doppio senso di circolazione all'interno di una carreggiata." ;
