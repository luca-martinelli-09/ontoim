    skos:broader <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.9> ;
    skos:definition "Indica una officina meccanica o similari lungo la viabilità extraurbana." ;
    skos:example "https://it.wikipedia.org/wiki/File:Italian_traffic_signs_-_riparazioni.svg" ;
    skos:inScheme <https://w3id.org/ontoim/controlled-vocabulary/road-signal-types> ;
    skos:prefLabel "Assistenza meccanica" .

<https://w3id.org/ontoim/controlled-vocabulary/road-signal-types/V.3.9.20> a skos:Concept,
        ontoim:RoadSignalType ;
